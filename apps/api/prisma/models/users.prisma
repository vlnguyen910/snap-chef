enum UserRoles {
    USER
    MODERATOR
    ADMIN
}

model User {
    id         String    @id @default(cuid()) 
    email      String    @unique
    password   String
    username   String
    role       UserRoles
    avatar_url String?   
    is_active  Boolean  @default(true)
    bio        String?
    create_at  DateTime @default(now())

    //Relations
    Recipe     Recipe[]
}


